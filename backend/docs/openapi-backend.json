  "paths": {
    "/backend/help-assistant/": {
      "post": {
        "description": "Completion request for the help assistant",
        "parameters": [
          {
            "description": "A custom session id to be used to keep track of the chat history.<br>Only alphanumeric characters, underscores, and hyphens are accepted. The minmum and maximum lengths 5 and 50 characters.",
            "in": "header",
            "name": "X-Session-Id",
            "required": false,
            "schema": {
              "maxLength": 50,
              "minLength": 5,
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/HelpAssistant"
              }
            }
          }
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "code": {
                      "type": "integer"
                    },
                    "data": {
                      "$ref": "#/components/schemas/CompletionsResponse"
                    },
                    "message": {
                      "type": "string"
                    },
                    "time": {
                      "type": "integer"
                    }
                  },
                  "type": "object"
                }
              }
            },
            "description": "Successful response"
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "summary": "Help Assistant Completion",
        "tags": [
          "Help Assistant"
        ]
      }
    },
